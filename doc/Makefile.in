@ENABLE_PYEXT_TRUE@@WITH_PDFLATEX_TRUE@    $(PYTHON) $(srcdir)/mk-analysis-latex || true
@ENABLE_PYEXT_FALSE@@WITH_PDFLATEX_TRUE@analyses.bib analyses.tex:
@ENABLE_PYEXT_FALSE@@WITH_PDFLATEX_TRUE@	> analyses.tex
@ENABLE_PYEXT_FALSE@@WITH_PDFLATEX_TRUE@	> analyses.bib

@WITH_PDFLATEX_TRUE@rivet-manual.pdf : rivet-manual.tex $(LATEXSUBSOURCES) analyses.tex refs.bib analyses.bib
@WITH_PDFLATEX_TRUE@	$(LATEX) $<; true
@WITH_PDFLATEX_TRUE@	egrep -c $(RERUNBIB) rivet-manual.log && (bibtex rivet-manual && cp rivet-manual.toc rivet-manual.toc.bak && $(LATEX) $<); true
@WITH_PDFLATEX_TRUE@	for i in `seq 5`; do if egrep $(RERUN) rivet-manual.log; then echo "LaTeX re-run $i"; cp rivet-manual.toc rivet-manual.toc.bak; $(LATEX) $<; else break; fi; done; true
@WITH_PDFLATEX_TRUE@	if cmp -s rivet-manual.toc rivet-manual.toc.bak; then true; else $(LATEX) $<; true; fi
@WITH_PDFLATEX_TRUE@	rm -f rivet-manual.toc.bak; true

@WITH_PDFLATEX_TRUE@paper.pdf : paper.tex $(LATEXSUBSOURCES) analyses.tex refs.bib analyses.bib
@WITH_PDFLATEX_TRUE@	$(LATEX) $<; true
@WITH_PDFLATEX_TRUE@	egrep -c $(RERUNBIB) paper.log && (bibtex paper && cp paper.toc paper.toc.bak && $(LATEX) $<); true
@WITH_PDFLATEX_TRUE@	for i in `seq 5`; do if egrep $(RERUN) paper.log; then echo "LaTeX re-run $i"; cp paper.toc paper.toc.bak; $(LATEX) $<; else break; fi; done; true
@WITH_PDFLATEX_TRUE@	if cmp -s paper.toc paper.toc.bak; then true; else $(LATEX) $<; true; fi
@WITH_PDFLATEX_TRUE@	rm -f paper.toc.bak; true

